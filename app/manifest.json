{
    "manifest_version": 2,
    "name": "Delicious Fuzzy Search",
    "description": "Allows you to fuzzy search your Delicious bookmarks.",
    "version": "1.5.0",
    "author": "Michael Wurster",
    "homepage_url": "https://miwurster.github.io/chrome-delicious-fuzzy-search",
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://api.delicious.com https://api.del.icio.us; object-src 'self'",
    "options_page": "options.html",
    "content_scripts": [
        {
            "matches": [
                "http://miwurster.github.io/chrome-delicious-fuzzy-search/robots.txt*"
            ],
            "js": [
                "js/components/oauth2-interceptor.js"
            ],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [
        "options.html"
    ],
    "chrome_url_overrides": {
        "newtab": "tab.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "24": "icons/icon24.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "64": "icons/icon64.png",
        "72": "icons/icon72.png",
        "96": "icons/icon96.png",
        "128": "icons/icon128.png",
        "256": "icons/icon256.png"
    }
}